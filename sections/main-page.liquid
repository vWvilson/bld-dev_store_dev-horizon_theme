<!-- /templates/page.liquid --> 
{{ 'pages.css' | asset_url | stylesheet_tag }}
<div class="container">
  <div class="page-container">
    <h1>{{ page.title }}</h1>
    {% comment %}
      Regular page content goes here.
    {% endcomment %}
    <div class="rte">
      {% include 'shortcode' load: page.content %} 
    </div>
  </div>
</div> 
{% schema %}
{
  "name": "t:sections.main-page.name",
  "tag": "section",
  "class": "spaced-section"
}
{% endschema %}
