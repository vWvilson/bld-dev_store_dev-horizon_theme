              "label": "t:sections.section-main.blocks.tab.settings.product_layout.options__4.label"
            },
            {
              "value": "awq_onimage",
              "label": "t:sections.section-main.blocks.tab.settings.product_layout.options__5.label"
            },
            {
              "value": "outimage",
              "label": "t:sections.section-main.blocks.tab.settings.product_layout.options__6.label"
            },
            {
              "value": "onimage",
              "label": "t:sections.section-main.blocks.tab.settings.product_layout.options__7.label"
            },
            {
              "value": "onimage2",
              "label": "t:sections.section-main.blocks.tab.settings.product_layout.options__8.label"
            },
