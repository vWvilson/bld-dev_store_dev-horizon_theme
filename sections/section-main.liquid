            }
          ],
          "default": "1"
        },
        {
          "type": "select",
          "id": "slide_pagination",
          "label": "t:sections.section-main.blocks.slideshow.settings.slide_pagination.label",
          "info": "t:sections.section-main.blocks.slideshow.settings.slide_pagination.info",
          "options": [
            {
              "value": "1",
              "label": "t:sections.section-main.blocks.slideshow.settings.slide_pagination.options__1.label"
            },
            {
              "value": "0",
              "label": "t:sections.section-main.blocks.slideshow.settings.slide_pagination.options__2.label"
            }
          ],
          "default": "1"
        },
        {
          "type": "select",
          "id": "page_style",
          "label": "t:sections.section-main.blocks.slideshow.settings.page_style.label", 
          "options": [
            {
              "value": "0",
              "label": "t:sections.section-main.blocks.slideshow.settings.page_style.options__1.label"
            },
            {
              "value": "1",
              "label": "t:sections.section-main.blocks.slideshow.settings.page_style.options__2.label"
            }
          ],
          "default": "1"
        } 
      ]
    },
    {
      "type": "slide",
      "name": "t:sections.section-main.blocks.slide.name",
      "settings": [
        {
          "type": "image_picker",
          "id": "background_image",
          "label": "t:sections.section-main.blocks.slide.settings.background_image.label"
        },
        {
          "type": "select",
          "id": "background_image_use",
          "label": "t:sections.section-main.blocks.slide.settings.background_image_use.label",
          "info": "t:sections.section-main.blocks.slide.settings.background_image_use.info",
          "options": [
            {
              "value": "1",
              "label": "t:sections.section-main.blocks.slide.settings.background_image_use.options__1.label"
            },
            {
              "value": "0",
              "label": "t:sections.section-main.blocks.slide.settings.background_image_use.options__2.label"
            }
          ],
          "default": "1"
        },
        {
          "type": "image_picker",
          "id": "image",
          "label": "t:sections.section-main.blocks.slide.settings.image.label"
        },
        {
          "type": "select",
          "id": "image_use",
          "label": "t:sections.section-main.blocks.slide.settings.image_use.label",
          "info": "t:sections.section-main.blocks.slide.settings.image_use.info",
          "options": [
            {
              "value": "1",
              "label": "t:sections.section-main.blocks.slide.settings.image_use.options__1.label"
            },
            {
              "value": "0",
              "label": "t:sections.section-main.blocks.slide.settings.image_use.options__2.label"
            }
          ],
          "default": "1"
        },
        {
          "type": "color",
          "id": "lazy_back_color",
          "label": "t:sections.section-main.blocks.slide.settings.lazy_back_color.label",
          "default": "#eee"
        },  
        {
          "type": "html",
          "id": "content",
          "label": "t:sections.section-main.blocks.slide.settings.content.label",
          "info": "t:sections.section-main.blocks.slide.settings.content.info"
        }
      ]
    },
    {
      "type": "tab",
      "name": "t:sections.section-main.blocks.tab.name", 
      "settings": [ 
    	{
          "type": "header",
          "content": "t:sections.section-main.blocks.tab.settings.header__1.content"
        },
        {
          "type": "text",
          "id": "margin_bottom",
          "label": "t:sections.section-main.blocks.tab.settings.margin_bottom.label",
		  "default": "24px" 
        },
        {
          "type": "select",
          "id": "view_mode",
          "label": "t:sections.section-main.blocks.tab.settings.view_mode.label",
